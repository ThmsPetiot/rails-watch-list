<h1><%= @list.name %></h1>
<ul>
  <% @list.movies.each do |movie| %>
  <li><%= movie.title %> <%= link_to "Delete", list_bookmark_path(@list, movie), data: { turbo_method: :delete, turbo_confirm: "Are you sure ?" } %></li>
  <% end %>
</ul>

<%= link_to "Ajouter un film", new_list_bookmark_path(@list) %>
